(Code new: 'List by: 0.') run.
☞ x := Code new: '✎ write: \'hello there\', end.'.
x run.
✎ write: '+ 3 to Number gives: ? ' ? (Number + 3), end.
✎ write: 'Adding 3 to Number gives: ? ' ? (Number add: 3), end.
✎ write: ('Appending something to String gives: [this]' [this]: (String append: 'something')), end.
✎ write: Boolean string, end.
✎ write: Code string, end.
✎ write: 'Can I execute a code block without assigning code?', end.
✎ write: (Code * 10), end.
✎ write: (Code true: {}), end.
✎ write: (Object done), end.
✎ write: ('This <path> has no file' <path>: File path), end.
✎ write: List count, end.
☞ dictionary := (File new: 'dictionary.h') read.
☞ words := List new.
☞ things := List new.
☞ counter := 0.
(dictionary split: '↵') each: { :number :line
	line pattern: '"([^"]+)"' process: { :match
		(counter > 17) true: { words add: (match position: 1). }.
		((counter > 5) & (counter < 18)) true: { things add: (match position: 1). }.
		counter add: 1.
	}.
}.

things each: { :n :thing
	words each: { :i :word
		☞ task := '<object> <message>'.
		
		(thing = 'Code' & word = 'new:') continue.
		(thing = 'Program' & word = 'ask') continue.
		
		task := task <object>:  thing.
		task := task <message>: word.
		
		
		task := task replace: ':' with: ': 0 '.
		(word length = 1) true: { task append: ' 0'. }.
		task append: '.'.
		
		✎ write: task, end.
		☞ code := Code new: task.
		code catch: { :error ✎ write: 'error', end. }, run.
	}.
}.

