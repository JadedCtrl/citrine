'SINGLE_LANGUAGE'.


☞ a := True.



a on: 'string' do: {
	☞ answer := Nil.
	( ⛏ = True ) true: {
		answer := 'Yep'.
	}, else: {
		answer := 'Nope'.
	}.
	↲ answer.
}.

a on: 'number' do: {
	☞ answer := Nil.
	( ⛏ = True ) true: {
		answer := 10.
	}, else: {
		answer := 20.
	}.
	↲ answer.
}.

☞ b := 666.

b on: 'string' do: {
	↲ 'Devil'.
}.

b on: 'boolean' do: {
	↲ False.
}.


✎ write: a, end.  
✎ write: (90 + a), end. 
✎ write: b, end. 
✎ write: (True & b), end. 

