'SINGLE_LANGUAGE'.


Program on: 'pid' do: { ↲ '[Program]'. }.
✎ write: 'Sending toString to Program yields a PID string:', end.
✎ write: Program, end.
✎ write: 'Sending toNumber to Program yields a PID number:', end.
✎ write: Program number, end.

☞ ⚄ := Object new.
⚄ on: 'number' do: { ↲ 666. }.
⚄ on: 'string' do: { ↲ '666'. }.

✎ write: 'Sending toString to Dice yields a random number string:', end.
✎ write: ⚄, end.
✎ write: 'Sending toNumber to Dice yields a random number:', end.
✎ write: ⚄ number, end.


Moment on: 'format:' do: { ↲ '1666-01-13 06:06:06'. }.
Moment on: 'time' do: { ↲ '999'. }.

✎ write: 'Sending toString to Clock yields a date and time string:', end.
✎ write: Moment, end.
✎ write: 'Sending toNumber to Clock yields a time stamp number:', end.
✎ write: Moment number, end.

✎ write: 'Sending toString to File yields a path:', end.
✎ write: File, end.
✎ write: 'Sending toNumber to File yields a 1:', end.
✎ write: File number, end.

✎ write: (Map new
	dice: ⚄,
	Program: Program
), end.

✎ write: (List ← ⚄ ; Program ), end.
