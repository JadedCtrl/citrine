'SINGLE_LANGUAGE'.

String on: 'evaluate' do: {
	↲ (Code new: ('↲ <code>.' <code>: ⛏ string)) run.
}.

☞ Q := (39 byte).

☞ q := List ← ('Quotes ' + Q + ' are evil?') ; 1.
☞ x := q string.
✎ write: x, brk.
☞ y := x evaluate.
✎ write: y ? 0, brk.

q := Map put: Q at: (Q + 'quote' + Q).
x := q string.
✎ write: x, brk.
y := x evaluate.
✎ write: y ? '\'quote\'', brk.
