'SINGLE_LANGUAGE'.
String on: 'evaluate' do: {
	↲ (Code new: ('↲ <code>.' <code>: ⛏ string)) run.
}.

☞ a := List ← 1 ; 2 ; 3.
☞ b := (a string).

✎ write: b, brk. 
b := b evaluate.

✎ write: b ? 2, brk. 

✎ write: (( List ← 1 ; 2 ; 3 )), brk. 

✎ write: ((Map new) put: 1 at: 2), brk. 

☞ x := ( (List ← 1 ; ( List ← 9 ) ; True ) ) string.
✎ write: x, brk. 
x := x evaluate.

✎ write: x ? 1 ? 0, brk. 

☞ y := (Map new) put: (List ← 1) at: (List ← 2).
✎ write: y, brk. 

y := y string evaluate.

✎ write: (y at: (List ← 2) string ), brk.

✎ write: (List new), brk. 
✎ write: (Map new), brk. 
