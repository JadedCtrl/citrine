
#numbers, pass by value

a = 3.
b = a.
a dec: 1.
Pen write: ' a = ' + a.
Pen write: ' b = ' + b.

a = 3.
b = a + 4.
Pen write: ' a = ' + a.
Pen write: ' b = ' + b.

x = 3.
y = x.
x inc: 10.
y dec: 1.
Pen write: ' x = ' + x.
Pen write: ' y = ' + y.

