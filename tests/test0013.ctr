Animal = Object new.
Animal method: 'make' does: {\
	
	animal = Object new.
	
	animal method: 'makeSound' does: {\
		Console write: '?'.
	}.

	^animal.
}.

Cat = Object new.
Cat method: 'make' does: {\
	cat = Animal make.
	
	cat override: 'makeSound' does: {\
		Console write: 'meow!!'.
		me overridden-makeSound.
	}.
	
	^cat.
}.

CatObject = Cat make.
tom = Object new basedOn: CatObject.
tom makeSound.
