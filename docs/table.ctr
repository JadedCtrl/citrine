☞ dict := Program argument: 2.
☞ file := File new: dict.
☞ data := file read.
☞ lines := data split: '\n'.
✎ write: '"word";"translation"', end.
lines each: { :number :line
	☞ parts := line split: '"'.
	☞ word := parts position: 1.
	☞ translation := parts position: 3.
	✎ write: (
		'"<word>";"<translation>"'
		<word>: word,
		<translation>: translation
	), end.
	
}.

